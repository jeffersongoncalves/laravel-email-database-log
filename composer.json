{
    "name": "jeffersonsimaogoncalves/laravel-email-database-log",
    "description": "A simple database logger for all outgoing emails sent by Laravel website.",
    "keywords": ["laravel", "markdown", "mail"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jefferson Simão Gonçalves",
            "email": "gerson.simao.92@gmail.com"
        },
        {
            "name": "Alexander Shvets",
            "email": "neochief@shvetsgroup.com"
        },
        {
            "name": "Spaan Productions",
            "email": "info@spaanproductions.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "doctrine/dbal": "^3.3",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-0": {
            "JeffersonSimaoGoncalves\\LaravelEmailDatabaseLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeffersonSimaoGoncalves\\LaravelEmailDatabaseLog\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JeffersonSimaoGoncalves\\LaravelEmailDatabaseLog\\LaravelEmailDatabaseLogServiceProvider"
            ]
        }
    }
}
